# ExperimentalPlatform
[Unit]
Description=Protonet SMB

[Path]
PathModified=/etc/protonet/hostname
PathModified=/data/samba/etc/smb.conf
PathModified=/data/samba/etc/smbpasswd
PathModified=/data/samba/etc/user_volumes.conf
PathModified=/data/samba/extrausers/passwd
PathModified=/data/samba/extrausers/shadow

[Install]
WantedBy=multi-user.target
